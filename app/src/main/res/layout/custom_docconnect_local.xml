<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp">

        <RelativeLayout
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/task_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="Android" />
        </RelativeLayout>

        <TextView
            android:id="@+id/documentType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android" />

        <TextView
            android:id="@+id/publishCategory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android" />

        <TextView
            android:id="@+id/briefContentFull"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android" />

        <TextView
            android:id="@+id/docNumberFull"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android" />

        <TextView
            android:id="@+id/numberOfSymbol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android" />

        <TextView
            android:id="@+id/briefContent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android" />

        <TextView
            android:id="@+id/publishDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Android" />

        <com.newsaigonsoft.neoegov.CustomViewListExpand.NonScrollListView
            android:id="@+id/list_attach_doc_connect"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></com.newsaigonsoft.neoegov.CustomViewListExpand.NonScrollListView>


    </LinearLayout>
</LinearLayout>